    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "mailersend": "^2.5.0",
    "nodemailer": "^6.9.4",
    "pdf-lib": "^1.17.1",
    "redis": "^4.6.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
